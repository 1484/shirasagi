<dl class="see mod-attribute">
  <dt><%= t("facility.attribute.name") %><%= @model.tt :attributes %></dt>
  <dd class="wide" style="line-height: 2.5">
    <%= f.hidden_field "attribute_ids[]", value: "" %>
    <% if @cur_node.present? %>
      <% @cur_node.st_attributes.each do |cate| %>
        <label>
          <%= f.check_box("attribute_ids[]", {}, cate._id) %>
          <%= cate.name %>
        </label>
      <% end %>
    <% end %>
  </dd>
</dl>
