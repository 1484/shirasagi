<dl class="see ss-addon-user-group-history">
  <dt><%= t 'modules.addons.ss/user_group_history' %></dt>
  <dd>
    <% @item.group_histories.each do |item| %>
      <%= tryb { item.created.strftime('%Y/%m/%d %H:%M') } %>
      - <%= item.groups.map(&:section_name).join(', ').presence || '-' %><br />
    <% end %>
  </dd>
</dl>
