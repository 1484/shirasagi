<%= f.ss_field_set :default_release_plan_state, value: @item.label(:default_release_plan_state), disabled: true %>

<% if @item.default_release_days_after %>
  <%= f.ss_field_set :default_release_days_after, value: "#{@item.default_release_days_after} #{t("ss.units.days_after")}", disabled: true %>
<% end %>

<% if @item.default_close_days_after %>
  <%= f.ss_field_set :default_close_days_after, value: "#{@item.default_close_days_after} #{t("ss.units.days_after")}", disabled: true %>
<% end %>

<% if @item.close_days_before %>
  <%= f.ss_field_set :close_days_before, value: "#{@item.close_days_before} #{t("ss.units.days_before")}", disabled: true %>
<% end %>
