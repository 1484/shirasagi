<%= f.ss_field_set :page_layout_id, value: tryb { @item.page_layout.name }, disabled: true %>
<%= f.ss_field_set :order, disabled: true %>
<%= f.ss_field_set :shortcut, value: @item.label(:shortcut), disabled: true %>
<%= f.ss_field_set :view_route, disabled: true do %>
  <% if @item.view_route.present? %>
    <%= f.text_field :view_route, value: Cms::Node.new.route_options.map(&:reverse).to_h[@item.view_route] %>
  <% end %>
<% end %>
