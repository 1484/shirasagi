<table class="index">
  <thead>
    <tr>
      <th><%= @model.t :date %></th>
      <th><%= @model.t :link_url %></th>
      <th><%= @model.t :count %></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
    <tr>
      <td><%= item.date %></td>
      <td><%= item.link_url %></td>
      <td><%= item.count %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @items if @items.try(:current_page) %>
